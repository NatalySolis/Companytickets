@model IEnumerable<ProyectoSistemaTickets.Models.ticket>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.descripcion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.idcuenta)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.idpriori)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.idservicio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.fecha_crear)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.descripcion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.idcuenta)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.idpriori)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.idservicio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.fecha_crear)
            </td>
            <td>
                <a asp-action="Details" asp-route-id="@item.idticket">Details</a> |
            </td>
        </tr>
}
    </tbody>
</table>
